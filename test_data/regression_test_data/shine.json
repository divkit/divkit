{
  "templates": {},
  "card": {
    "log_id": "sample_card",
    "variables": [
      {
        "name": "shine_cycles_count",
        "type": "integer",
        "value": 0
      }
    ],
    "states": [
      {
        "state_id": 0,
        "div": {
          "type": "container",
          "orientation": "vertical",
          "alignment_horizontal": "center",
          "content_alignment_horizontal": "center",
          "margins": {
            "top": 24,
            "bottom": 24
          },
          "items": [
            {
              "type": "image",
              "preview": "",
              "image_url": "empty://",
              "width": {
                "type": "fixed",
                "value": 100
              },
              "height": {
                "type": "fixed",
                "value": 100
              },
              "extensions": [
                {
                  "id": "shine",
                  "params": {
                    "enabled": true,
                    "cycle_count": 3,
                    "interval": 1000,
                    "delay": 1000,
                    "duration": 1000,
                    "angle": 0,
                    "colors": [
                      "#0055C776",
                      "#FF5EDB82",
                      "#0055C776"
                    ],
                    "locations": [
                      0,
                      0.5,
                      1
                    ],
                    "on_cycle_start_actions": [
                      {
                        "log_id": "shine_counter_increase",
                        "url": "div-action://set_variable?name=shine_cycles_count&value=@{shine_cycles_count+1}"
                      }
                    ]
                  }
                }
              ]
            },
            {
              "type": "text",
              "text": "Cycle counter: @{shine_cycles_count}",
              "width": {
                "type": "wrap_content"
              }
            }
          ]
        }
      }
    ]
  }
}
