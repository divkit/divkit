        "right": 4,
        "bottom": 4,
        "top": 4
      },
      "paddings": {
        "top": 4,
        "right": 2,
        "left": 2
      },
      "border": {
        "corner_radius": 32,
        "unit": "dp"
      },
      "background": [
        {
          "type": "solid",
          "color": "#0e000000"
        }
      ],
      "orientation": "vertical"
    },
    "rive_animation_card": {
      "type": "rive_card",
      "items": [
        {
          "type": "card_title",
          "$text": "rive_card_text"
        },
        {
          "type": "rive_custom",
          "$custom_props": "rive_props"
        }
      ]
    }
