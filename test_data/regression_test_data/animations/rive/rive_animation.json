        "bottom": 8
      },
      "column_count": 2
    },
    "rive_custom": {
      "type": "custom",
      "custom_type": "rive_animation",
      "width": {
        "type": "match_parent"
      },
      "height": {
        "type": "match_parent"
      },
      "margins": {
        "top": 32,
        "bottom": 16,
        "left": 16,
        "right": 16
      },
      "alignment_vertical": "top",
      "alignment_horizontal": "center"
    },
    "card_title": {
