{
  "templates": {
    "picture": {
      "type": "image",
      "placeholder_color": "#bdc3c7"
    },
    "title": {
      "type": "text",
      "margins": {
        "left": 24,
        "top": 24,
        "right": 24,
        "bottom": 24
      },
      "font_size": 24,
      "font_weight": "medium",
      "text_color": "#FFFFFF"
    },
    "button": {
      "type": "state",
      "$div_id": "button_id",
      "margins": {
        "left": 24,
        "top": 24,
        "right": 24,
        "bottom": 24
      },
      "states": [
        {
          "state_id": "inactive",
          "div": {
            "type": "image",
            "$id": "inactive_state_id",
            "width": {
              "type": "match_parent"
            },
            "height": {
              "type": "match_parent"
            },
            "preload_required": 1,
            "$image_url": "inactive_icon_url",
            "$actions": "inactive_actions",
            "action_animation": {
              "name": "no_animation"
            },
            "transition_in": {
              "type": "scale",
              "scale": 0.8,
              "duration": 150,
              "start_delay": 150,
              "interpolator": "ease_in_out"
            },
            "transition_out": {
              "type": "scale",
              "scale": 0.8,
              "duration": 150,
              "interpolator": "ease_in_out"
            }
          }
        },
        {
          "state_id": "active",
          "div": {
            "type": "image",
            "$id": "active_state_id",
            "width": {
              "type": "match_parent"
            },
            "height": {
              "type": "match_parent"
            },
            "preload_required": 1,
            "$image_url": "active_icon_url",
            "$actions": "active_actions",
            "action_animation": {
              "name": "no_animation"
            },
            "transition_in": {
              "type": "scale",
              "scale": 0.8,
              "duration": 150,
              "start_delay": 150,
              "interpolator": "ease_in_out"
            },
            "transition_out": {
              "type": "scale",
              "scale": 0.8,
              "duration": 150,
              "interpolator": "ease_in_out"
            }
          }
        }
      ]
    },
    "card": {
      "type": "container",
      "background": [
        {
          "type": "solid",
          "color": "#1e1e1e"
        }
      ],
      "orientation": "overlap"
    }
  },
  "card": {
    "log_id": "transition/state_transition_data_change",
    "transition_animation_selector": "data_change",
    "states": [
      {
        "state_id": 0,
        "div": {
          "type": "card",
          "height": {
            "type": "fixed",
            "value": 320
          },
          "items": [
            {
              "type": "picture",
              "id": "picture",
              "width": {
                "type": "match_parent"
              },
              "height": {
                "type": "match_parent"
              },
              "preview": "data:image/png;base64,",
              "image_url": "https://yastatic.net/s3/home/yandex-app/div_demo/containers.png",
              "transition_in": {
                "type": "fade",
                "duration": 500,
                "interpolator": "ease_out"
              }
            },
            {
              "type": "title",
              "id": "title",
              "width": {
                "type": "wrap_content"
              },
              "height": {
                "type": "wrap_content"
              },
              "alignment_horizontal": "left",
              "alignment_vertical": "bottom",
              "text": "Image title",
              "transition_in": {
                "type": "slide",
                "edge": "right",
                "duration": 500,
                "interpolator": "ease_out"
              }
            },
            {
              "type": "button",
              "button_id": "button",
              "transition_animation_selector": "data_change",
              "inactive_state_id": "button_inactive",
              "active_state_id": "button_active",
              "width": {
                "type": "fixed",
                "value": 48
              },
              "height": {
                "type": "fixed",
                "value": 48
              },
              "alignment_horizontal": "right",
              "alignment_vertical": "top",
              "inactive_icon_url": "https://s3.mds.yandex.net/mobile-yandex-client-android/divkit/images/icons/star_outline.png",
              "active_icon_url": "https://s3.mds.yandex.net/mobile-yandex-client-android/divkit/images/icons/star_fill.png",
              "inactive_actions": [
                {
                  "log_id": "press_inactive_button",
                  "url": "div-action://set_state?state_id=0/button/active"
                }
              ],
              "active_actions": [
                {
                  "log_id": "press_active_button",
                  "url": "div-action://set_state?state_id=0/button/inactive"
                }
              ]
            }
          ]
        }
      }
    ]
  }
}
