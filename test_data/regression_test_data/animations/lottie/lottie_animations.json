    "lottie_grid": {
      "type": "grid",
      "paddings": {
        "left": 8,
        "top": 8,
        "right": 8,
        "bottom": 8
      },
      "column_count": 2
    },
    "lottie_image": {
      "type": "image",
      "aspect": {
        "ratio": 0.8
      },
      "content_alignment_horizontal": "center",
      "content_alignment_vertical": "center"
    },
    "lottie_gif": {
      "type": "gif",
      "aspect": {
        "ratio": 0.8
      },
      "content_alignment_horizontal": "center",
      "content_alignment_vertical": "center"
    },
    "card_title": {
      "type": "text",
      "text_alignment_horizontal": "center",
      "text_alignment_vertical": "center",
      "font_size": 14,
      "paddings": {
        "top": 8
      },
      "font_weight": "medium",
      "line_height": 18
    },
    "lottie_card": {
      "type": "container",
      "width": {
        "type": "fixed",
        "value": 150
      },
      "height": {
        "type": "fixed",
        "value": 250
      },
      "margins": {
        "left": 4,
        "right": 4,
        "bottom": 4,
        "top": 4
      },
      "paddings": {
        "top": 4,
        "right": 2,
        "left": 2
      },
      "border": {
        "corner_radius": 32,
        "unit": "dp"
      },
      "background": [
        {
          "type": "solid",
          "color": "#0e000000"
        }
      ],
      "orientation": "vertical"
    },
    "lottie_gif_card": {
      "type": "lottie_card",
      "items": [
        {
          "type": "card_title",
          "$text": "lottie_image_text"
        },
        {
          "type": "lottie_gif",
          "$id": "image_id",
          "scale": "fit",
          "extensions": [
            {
              "id": "lottie",
              "$params": "lottie_params"
            }
          ],
          "gif_url": "https://i.gifer.com/VhdJ.gif"
        }
      ]
    }
  },
