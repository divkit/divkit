{
  "templates": {
    "tested_video": {
      "type": "video",
      "id": "bears_video",
      "preview": "",
      "video_sources": [
        {
          "type": "video_source",
          "$url": "video_url",
          "mime_type": "video/mp4"
        }
      ],
      "preload_required": true,
      "repeatable": false,
      "autostart": false,
      "muted": true,
      "elapsed_time_variable": "video_time",
      "height": {
        "type": "fixed",
        "value": 320
      },
      "background": [
        {
          "type": "solid",
          "color": "#000000"
        }
      ],
      "resume_actions": [
        {
          "log_id": "video_playing",
          "url": "div-action://set_state?state_id=0/play_signal/play_called"
        }
      ],
      "pause_actions": [
        {
          "log_id": "video_paused",
          "url": "div-action://set_state?state_id=0/pause_signal/pause_called"
        }
      ],
      "buffering_actions": [
        {
          "log_id": "video_buffering",
          "url": "div-action://set_state?state_id=0/buffering_signal/buffering_called"
        }
      ],
      "end_actions": [
        {
          "log_id": "video_ended",
          "url": "div-action://set_state?state_id=0/end_signal/end_called"
        }
      ],
      "fatal_actions": [
        {
          "log_id": "video_error",
          "url": "div-action://set_state?state_id=0/fatal_signal/fatal_called"
        }
      ]
    },
    "control_button": {
      "type": "image",
      "aspect": {
        "ratio": 1
      },
      "scale": "fit",
      "width": {
        "type": "fixed",
        "value": 44
      }
    },
    "signal_block": {
      "type": "container",
      "border": {
        "corner_radius": 16
      },
      "margins": {
        "top": 10,
        "bottom": 10,
        "left": 20,
        "right": 20
      },
      "items": [
        {
          "type": "text",
          "$text": "text",
          "paddings": {
            "right": 10,
            "left": 10,
            "top": 10,
            "bottom": 10
          },
          "font_size": 14
        }
      ]
    },
    "bad_signal": {
      "type": "signal_block",
      "background": [
        {
          "type": "solid",
          "color": "#ff4040"
        }
      ]
    },
    "good_signal": {
      "type": "signal_block",
      "background": [
        {
          "type": "solid",
          "color": "#42e73a"
        }
      ]
    }
  },
  "card": {
    "log_id": "video",
    "variables": [
      {
        "name": "video_time",
        "type": "integer",
        "value": 0
      }
    ],
    "states": [
      {
        "state_id": 0,
        "div": {
          "type": "container",
          "orientation": "vertical",
          "items": [
            {
              "type": "state",
              "id": "video",
              "states": [
                {
                  "state_id": "normal",
                  "div": {
                    "type": "tested_video",
                    "video_url": "https://yastatic.net/s3/home/divkit/bears.mp4"
                  }
                },
                {
                  "state_id": "crashed",
                  "div": {
                    "type": "tested_video",
                    "video_url": "..."
                  }
                }
              ]
            },
            {
              "type": "text",
              "text": "Time elapsed: @{video_time}"
            },
            {
              "type": "slider",
              "width": {
                "type": "match_parent"
              },
              "margins": {
                "top": 16,
                "left": 12,
                "right": 12
              },
              "max_value": 30000,
              "min_value": 0,
              "thumb_value_variable": "video_time",
              "thumb_style": {
                "type": "shape_drawable",
                "color": "#FFCC00",
                "stroke": {
                  "color": "#ffffff",
                  "width": 3
                },
                "shape": {
                  "type": "rounded_rectangle",
                  "item_height": {
                    "type": "fixed",
                    "value": 32
                  },
                  "item_width": {
                    "type": "fixed",
                    "value": 32
                  },
                  "corner_radius": {
                    "type": "fixed",
                    "value": 100
                  }
                }
              },
              "track_active_style": {
                "type": "shape_drawable",
                "color": "#FFCC00",
                "shape": {
                  "type": "rounded_rectangle",
                  "item_height": {
                    "type": "fixed",
                    "value": 6
                  }
                }
              },
              "track_inactive_style": {
                "type": "shape_drawable",
                "color": "#20000000",
                "shape": {
                  "type": "rounded_rectangle",
                  "item_height": {
                    "type": "fixed",
                    "value": 6
                  }
                }
              }
            },
            {
              "type": "container",
              "orientation": "horizontal",
              "width": {
                "type": "match_parent"
              },
              "height": {
                "type": "wrap_content"
              },
              "content_alignment_horizontal": "center",
              "items": [
                {
                  "type": "control_button",
                  "image_url": "https://yastatic.net/s3/home/divkit/rewind_button.png",
                  "margins": {
                    "right": 20
                  },
                  "actions": [
                    {
                      "log_id": "rewind",
                      "url": "div-action://set_variable?name=video_time&value=0"
                    }
                  ]
                },
                {
                  "type": "control_button",
                  "image_url": "https://yastatic.net/s3/home/divkit/play_button.png",
                  "margins": {
                    "right": 16
                  },
                  "paddings": {
                    "top": 3,
                    "bottom": 3
                  },
                  "actions": [
                    {
                      "log_id": "play",
                      "url": "div-action://video?id=bears_video&action=start"
                    }
                  ]
                },
                {
                  "type": "control_button",
                  "image_url": "https://yastatic.net/s3/home/divkit/pause_button.png",
                  "paddings": {
                    "top": 3,
                    "bottom": 3
                  },
                  "actions": [
                    {
                      "log_id": "pause",
                      "url": "div-action://video?id=bears_video&action=pause"
                    }
                  ]
                },
                {
                  "type": "container",
                  "width": {
                    "type": "fixed",
                    "value": 130
                  },
                  "border": {
                    "corner_radius": 16
                  },
                  "background": [
                    {
                      "type": "solid",
                      "color": "#604bd8"
                    }
                  ],
                  "margins": {
                    "left": 10
                  },
                  "items": [
                    {
                      "type": "text",
                      "text": "Crash video",
                      "text_color": "#ffffff",
                      "paddings": {
                        "right": 15,
                        "left": 15,
                        "top": 10,
                        "bottom": 10
                      },
                      "font_size": 18
                    }
                  ],
                  "actions": [
                    {
                      "log_id": "do_crash",
                      "url": "div-action://set_state?state_id=0/video/crashed"
                    }
                  ]
                }
              ]
            },
            {
              "type": "state",
              "id": "play_signal",
              "states": [
                {
                  "state_id": "play_not_called",
                  "div": {
                    "type": "bad_signal",
                    "text": "Play event not called"
                  }
                },
                {
                  "state_id": "play_called",
                  "div": {
                    "type": "good_signal",
                    "text": "Play event called"
                  }
                }
              ]
            },
            {
              "type": "state",
              "id": "pause_signal",
              "states": [
                {
                  "state_id": "pause_not_called",
                  "div": {
                    "type": "bad_signal",
                    "text": "Pause event not called"
                  }
                },
                {
                  "state_id": "pause_called",
                  "div": {
                    "type": "good_signal",
                    "text": "Pause event called"
                  }
                }
              ]
            },
            {
              "type": "state",
              "id": "buffering_signal",
              "states": [
                {
                  "state_id": "buffering_not_called",
                  "div": {
                    "type": "bad_signal",
                    "text": "Buffering event not called"
                  }
                },
                {
                  "state_id": "buffering_called",
                  "div": {
                    "type": "good_signal",
                    "text": "Buffering event called"
                  }
                }
              ]
            },
            {
              "type": "state",
              "id": "end_signal",
              "states": [
                {
                  "state_id": "end_not_called",
                  "div": {
                    "type": "bad_signal",
                    "text": "End event not called"
                  }
                },
                {
                  "state_id": "end_called",
                  "div": {
                    "type": "good_signal",
                    "text": "End event called"
                  }
                }
              ]
            },
            {
              "type": "state",
              "id": "fatal_signal",
              "states": [
                {
                  "state_id": "fatal_not_called",
                  "div": {
                    "type": "bad_signal",
                    "text": "Fatal event not called (Wait a little after press crash)"
                  }
                },
                {
                  "state_id": "fatal_called",
                  "div": {
                    "type": "good_signal",
                    "text": "Fatal event called"
                  }
                }
              ]
            }
          ]
        }
      }
    ]
  }
}
