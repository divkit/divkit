{
  "description": "Check subscriptions for getValue functions inside other getValue functions.",
  "div_data": {
    "card": {
      "log_id": "deep_nested_get_value",
      "variables": [
        {
          "name": "part_of_nested_name",
          "type": "string",
          "value": "1"
        },
        {
          "name": "nested_name_1",
          "type": "string",
          "value": "name_1"
        },
        {
          "name": "nested_name_2",
          "type": "string",
          "value": "name_2"
        },
        {
          "name": "name_1",
          "type": "string",
          "value": "var_1"
        },
        {
          "name": "name_2",
          "type": "string",
          "value": "var_2"
        },
        {
          "name": "name_3",
          "type": "string",
          "value": "var_3"
        },
        {
          "name": "var_1",
          "type": "integer",
          "value": 10
        },
        {
          "name": "var_2",
          "type": "integer",
          "value": 20
        },
        {
          "name": "var_3",
          "type": "integer",
          "value": 30
        },
        {
          "name": "var_4",
          "type": "integer",
          "value": 40
        }
      ],
      "variable_triggers": [
        {
          "condition": "@{getIntegerValue(getStringValue(getStringValue('nested_name_' + part_of_nested_name, ''), ''), 0) != 0}",
          "mode": "on_variable",
          "actions": [
            {
              "log_id": "check result",
              "typed": {
                "type": "set_variable",
                "variable_name": "result",
                "value": {
                  "type": "integer",
                  "value": "@{getIntegerValue(getStringValue(getStringValue('nested_name_' + part_of_nested_name, ''), ''), 0)}"
                }
              }
            }
          ]
        }
      ],
      "states": [
        {
          "state_id": 0,
          "div": {
            "type": "text",
            "text": "text"
          }
        }
      ]
    }
  },
  "cases": [
    {
      "div_actions": [
        {
          "log_id": "change part of nested name",
          "url": "div-action://set_variable?name=part_of_nested_name&value=2"
        },
        {
          "log_id": "change nested name",
          "url": "div-action://set_variable?name=nested_name_2&value=name_3"
        },
        {
          "log_id": "change name",
          "url": "div-action://set_variable?name=name_3&value=var_4"
        },
        {
          "log_id": "change value",
          "url": "div-action://set_variable?name=var_4&value=41"
        }
      ],
      "expected": [
        {
          "type": "variable",
          "variable_name": "result",
          "value": {
            "type": "integer",
            "value": 41
          }
        }
      ],
      "platforms": [
        "android",
        "web"
      ]
    }
  ]
}
