{
  "description": "Check subscription for getUrlValue with url fallback.",
  "div_data": {
    "card": {
      "log_id": "get_url_value_with_url_fallback",
      "variables": [
        {
          "name": "part_of_name",
          "type": "string",
          "value": "1"
        },
        {
          "name": "var_1",
          "type": "url",
          "value": "url1://initial"
        },
        {
          "name": "var_2",
          "type": "url",
          "value": "url2://initial"
        },
        {
          "name": "fallback",
          "type": "url",
          "value": "empty://"
        }
      ],
      "variable_triggers": [
        {
          "condition": "@{getUrlValue('var_' + part_of_name, fallback) != fallback}",
          "mode": "on_variable",
          "actions": [
            {
              "log_id": "check result",
              "typed": {
                "type": "set_variable",
                "variable_name": "result",
                "value": {
                  "type": "url",
                  "value": "@{getUrlValue('var_' + part_of_name, fallback)}"
                }
              }
            }
          ]
        }
      ],
      "states": [
        {
          "state_id": 0,
          "div": {
            "type": "text",
            "text": "text"
          }
        }
      ]
    }
  },
  "cases": [
    {
      "expected": [
        {
          "type": "variable",
          "variable_name": "result",
          "value": {
            "type": "url",
            "value": "url1://initial"
          }
        }
      ],
      "platforms": [
        "android",
        "ios",
        "web"
      ]
    },
    {
      "div_actions": [
        {
          "log_id": "change part of name",
          "url": "div-action://set_variable?name=part_of_name&value=2"
        }
      ],
      "expected": [
        {
          "type": "variable",
          "variable_name": "result",
          "value": {
            "type": "url",
            "value": "url2://initial"
          }
        }
      ],
      "platforms": [
        "android",
        "ios",
        "web"
      ]
    },
    {
      "div_actions": [
        {
          "log_id": "change value",
          "typed": {
            "type": "set_variable",
            "variable_name": "var_1",
            "value": {
              "type": "url",
              "value": "url1://changed"
            }
          }
        }
      ],
      "expected": [
        {
          "type": "variable",
          "variable_name": "result",
          "value": {
            "type": "url",
            "value": "url1://changed"
          }
        }
      ],
      "platforms": [
        "android",
        "ios",
        "web"
      ]
    },
    {
      "div_actions": [
        {
          "log_id": "change part of name",
          "url": "div-action://set_variable?name=part_of_name&value=2"
        },
        {
          "log_id": "change value",
          "typed": {
            "type": "set_variable",
            "variable_name": "var_2",
            "value": {
              "type": "url",
              "value": "url2://changed"
            }
          }
        }
      ],
      "expected": [
        {
          "type": "variable",
          "variable_name": "result",
          "value": {
            "type": "url",
            "value": "url2://changed"
          }
        }
      ],
      "platforms": [
        "android",
        "ios",
        "web"
      ]
    },
    {
      "div_actions": [
        {
          "log_id": "change part of name",
          "url": "div-action://set_variable?name=part_of_name&value=2"
        },
        {
          "log_id": "change old value",
          "typed": {
            "type": "set_variable",
            "variable_name": "var_1",
            "value": {
              "type": "url",
              "value": "url1://changed"
            }
          }
        }
      ],
      "expected": [
        {
          "type": "variable",
          "variable_name": "result",
          "value": {
            "type": "url",
            "value": "url2://initial"
          }
        }
      ],
      "platforms": [
        "android",
        "ios",
        "web"
      ]
    },
    {
      "div_actions": [
        {
          "log_id": "change another value",
          "typed": {
            "type": "set_variable",
            "variable_name": "var_2",
            "value": {
              "type": "url",
              "value": "url2://changed"
            }
          }
        },
        {
          "log_id": "change part of name",
          "url": "div-action://set_variable?name=part_of_name&value=2"
        }
      ],
      "expected": [
        {
          "type": "variable",
          "variable_name": "result",
          "value": {
            "type": "url",
            "value": "url2://changed"
          }
        }
      ],
      "platforms": [
        "android",
        "ios",
        "web"
      ]
    }
  ]
}
