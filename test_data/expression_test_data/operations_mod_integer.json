        {
          "type": "integer",
          "value": 2,
          "name": "b"
        }
      ],
      "platforms": [
        "android",
        "ios",
        "web"
      ]
    },
    {
      "expression": "@{a % -b}",
      "expected": {
        "type": "integer",
        "value": 1
      },
      "variables": [
        {
          "type": "integer",
          "value": 3,
          "name": "a"
        },
        {
          "type": "integer",
          "value": -2,
          "name": "b"
        }
      ],
      "platforms": [
        "android",
        "ios",
        "web"
      ]
    }
  ]
