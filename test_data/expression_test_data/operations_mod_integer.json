{
  "cases": [
    {
      "expression": "@{4 % 2}",
      "expected": {
        "type": "integer",
        "value": 0
      },
      "platforms": [
        "android",
        "ios",
        "web"
      ]
    },
    {
      "expression": "@{3000000000 % 2000000000}",
      "expected": {
        "type": "integer",
        "value": 1000000000
      },
      "platforms": [
        "android",
        "ios",
        "web"
      ]
    },
    {
      "expression": "@{3 % 2}",
      "expected": {
        "type": "integer",
        "value": 1
      },
      "platforms": [
        "android",
        "ios",
        "web"
      ]
    },
    {
      "expression": "@{2 % 0}",
      "expected": {
        "type": "error",
        "value": "Failed to evaluate [2 % 0]. Division by zero is not supported."
      },
      "platforms": [
        "android",
        "ios",
        "web"
      ]
    },
    {
      "expression": "@{0 % 2}",
      "expected": {
        "type": "integer",
        "value": 0
      },
      "platforms": [
        "android",
        "ios",
        "web"
      ]
    },
    {
      "expression": "@{-3 % 2}",
      "expected": {
        "type": "integer",
        "value": -1
      },
      "platforms": [
        "android",
        "ios",
        "web"
      ]
    },
    {
      "expression": "@{3 % -2}",
      "expected": {
        "type": "integer",
        "value": 1
      },
      "platforms": [
        "android",
        "ios",
        "web"
      ]
    },
    {
      "expression": "@{-3 % -2}",
      "expected": {
        "type": "integer",
        "value": -1
      },
      "platforms": [
        "android",
        "ios",
        "web"
      ]
    },
    {
      "expression": "@{a % b}",
      "expected": {
        "type": "integer",
        "value": 0
      },
      "variables": [
        {
          "type": "integer",
          "value": 4,
          "name": "a"
        },
        {
          "type": "integer",
          "value": 2,
          "name": "b"
        }
      ],
      "platforms": [
        "android",
        "ios",
        "web"
      ]
    },
    {
