      },
      "variables": [
        {
          "type": "integer",
          "value": 2,
          "name": "a"
        }
      ],
      "platforms": [
        "android",
        "ios",
        "web"
      ]
    },
    {
      "expression": "@{-a % b}",
      "expected": {
