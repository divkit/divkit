        "value": 2
      },
      "platforms": [
        "android",
        "ios",
        "web"
      ]
    },
    {
      "expression": "@{30000000000 - 10000000000}",
      "expected": {
        "type": "integer",
        "value": 20000000000
      },
      "platforms": [
        "android",
        "ios",
        "web"
      ]
    },
    {
      "expression": "@{3 - 1 - 2}",
