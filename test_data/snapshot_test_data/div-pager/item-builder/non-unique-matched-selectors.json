{
  "description": "Only the first matched selector should be used. Zero matched selectors means no error.",
  "platforms": [
    "android"
  ],
  "card": {
    "log_id": "prototypes",
    "variables": [
      {
        "name": "data",
        "type": "array",
        "value": [
          {
            "type": "first_type"
          },
          {
            "type": "second_type"
          }
        ]
      }
    ],
    "states": [
      {
        "state_id": 0,
        "div": {
          "type": "pager",
          "orientation": "horizontal",
          "layout_mode": {
            "type": "percentage",
            "page_width": {
              "type": "percentage",
              "value": 80
            },
            "width": {
              "type": "match_parent"
            },
            "height": {
              "type": "match_parent"
            }
          },
          "item_builder": {
            "type": "prototype",
            "data": "@{data}",
            "prototypes": [
              {
                "selector": "@{getStringFromDict(it, 'type') == 'first_type'}",
                "div": {
                  "type": "text",
                  "text": "First proto: @{getStringFromDict(it, 'type')}",
                  "border": {
                    "corner_radius": 16,
                    "stroke": {
                      "color": "#aaaaaa",
                      "width": 1
                    }
                  },
                  "paddings": {
                    "top": 12,
                    "bottom": 12,
                    "left": 12,
                    "right": 12
                  }
                }
              },
              {
                "selector": "@{getStringFromDict(it, 'type') == 'first_type'}",
                "div": {
                  "type": "text",
                  "text": "Second proto: @{getStringFromDict(it, 'type')}"
                }
              }
            ]
          }
        }
      }
    ]
  }
}
