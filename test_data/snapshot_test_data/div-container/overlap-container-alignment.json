{
  "description": "Overlap container alignmnet. Foreground text should be on the left and foreground, background text should be on the right and background.",
  "platforms": [
    "android",
    "ios",
    "web",
    "flutter"
  ],
  "templates": {
    "big_text": {
      "font_size": 11,
      "line_height": 12,
      "type": "text"
    }
  },
  "card": {
