{
  "description": "Svg images created by url/preview",
  "platforms": [
    "android",
    "web"
  ],
  "templates": {
    "test_block": {
      "type": "container",
      "width": {
        "type": "fixed",
        "value": 150
      }
    },
    "svg_card": {
      "type": "image",
      "height": {
        "type": "fixed",
        "value": 150
      },
      "margins": {
        "left": 10,
        "top": 20,
        "right": 10,
        "bottom": 10
      },
      "alignment_horizontal": "center"
    },
    "svg_card_text": {
      "type": "text",
      "text_alignment_horizontal": "center",
      "font_size": 18
    }
  },
  "card": {
    "log_id": "snapshot_test_card",
    "states": [
      {
        "state_id": 0,
        "div": {
          "type": "container",
          "layout_mode": "wrap",
          "orientation": "horizontal",
          "content_alignment_horizontal": "space-evenly",
          "items": [
            {
              "type": "test_block",
              "items": [
                {
                  "type": "svg_card",
                  "image_url": "https://yastatic.net/s3/home/divkit/svg-test-image.svg"
                },
                {
                  "type": "svg_card_text",
                  "text": "Svg image"
                }
              ]
            },
            {
              "type": "test_block",
              "items": [
                {
                  "type": "svg_card",
                  "image_url": "empty://",
                  "preview": "data:image/svg+xml;base64,"
                },
                {
                  "type": "svg_card_text",
                  "text": "Svg image (preview)"
                }
              ]
            }
          ]
        }
      }
    ]
  }
}
