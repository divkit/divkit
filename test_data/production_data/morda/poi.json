  "platforms": [
    "android",
    "ios",
    "web"
  ],
  "templates": {
    "title_text": {
      "type": "text",
      "paddings": {
        "left": 16,
        "top": 12,
        "right": 16
      },
      "font_size": 18,
      "font_weight": "medium",
      "line_height": 24,
      "text_color": "#CC000000"
    },
    "title_menu": {
      "type": "image",
      "width": {
        "type": "fixed",
        "value": 44
      },
      "height": {
        "type": "fixed",
        "value": 44
      },
      "image_url": "https://yastatic.net/s3/home/yandex-app/services_div/redesign/zen_menu_dark.3.png"
    },
    "footer": {
      "type": "text",
      "$text": "footer_text",
      "font_size": 12,
      "line_height": 16,
      "text_color": "#80000000",
      "margins": {
        "left": 16,
        "right": 16
      },
      "paddings": {
        "bottom": 12,
        "top": 12
      },
      "$action": "footer_action_link"
    },
    "poi_card": {
      "type": "container",
      "orientation": "vertical",
      "background": [{
        "type": "solid",
        "color": "#FFFFFF"
      }],
      "items": [
        {
          "type": "container",
          "orientation": "horizontal",
          "$items": "title_items"
        },
        {
          "type": "tabs",
          "switch_tabs_by_content_swipe_enabled": 0,
          "title_paddings": {
            "left": 12,
            "right": 12,
            "bottom": 8
          },
          "tab_title_style": {
            "font_weight": "medium"
          },
          "$items": "tab_items_link"
        },
        {
          "type": "footer"
        }
      ]
    },
    "star": {
      "type": "image",
      "width": {
        "type": "fixed",
        "value": 14
      },
      "height": {
        "type": "fixed",
        "value": 14
      }
    },
    "star_full": {
      "type": "star",
      "image_url": "https://avatars.mds.yandex.net/get-bass/787408/poi_48x48_ee9550bc195fdc5d7c1d281ea5d8d776320345e0a67b0663c4fdde14e194393b.png/orig"
    },
    "star_half": {
      "type": "star",
      "image_url": "https://avatars.mds.yandex.net/get-bass/469429/poi_48x48_188933e7030027690ed55b5614b60fa77e0e4b50b86dde48d166714096ed0b0e.png/orig"
    },
    "star_empty": {
      "type": "star",
      "image_url": "https://avatars.mds.yandex.net/get-bass/397492/poi_48x48_4ce4cec5ea8f8336bc3792a4899c1e9958531fcf9f8aabc4dd319ddaf5deafa0.png/orig"
    },
    "poi_gallery_item": {
      "type": "container",
      "orientation": "vertical",
      "width": {
        "type": "fixed",
        "value": 244
      },
      "height": {
        "type": "fixed",
        "value": 240,
        "unit": "sp"
      },
      "border": {
        "corner_radius": 6
      },
      "paddings": {
        "left": 12,
        "right": 12,
        "top": 16,
        "bottom": 14
      },
      "background": [
        {
          "type": "image",
          "content_alignment_vertical": "top",
          "$image_url": "background_url"
        },
        {
          "type": "gradient",
          "angle": 270,
          "colors": [
            "#00293445",
            "#293445"
          ]
        }
      ],
      "action": "poi_gallery_item_action_link",
      "items": [
        {
          "type": "text",
          "$text": "badge_text",
          "font_size": 13,
          "text_color": "#fff",
          "line_height": 16,
          "width": {
            "type": "wrap_content"
          },
          "max_lines": 1,
          "alignment_horizontal": "right",
          "background": [
            {
              "type": "solid",
              "color": "#3A4F71"
            }
          ],
          "border": {
            "corner_radius": 4
          },
          "paddings": {
            "left": 6,
            "right": 6,
            "top": 4,
            "bottom": 4
          }
        },
        {
          "type": "text",
          "$text": "place_category",
          "font_size": 11,
          "line_height": 12,
          "max_lines": 6,
          "letter_spacing": 0.75,
          "text_color": "#FFFFFF",
          "text_alignment_vertical": "bottom",
          "height": {
            "type": "match_parent",
            "weight": 1
          },
          "paddings": {
            "top": 8
          }
        },
        {
          "type": "text",
          "$text": "place_title",
          "font_size": 16,
          "text_color": "#FFFFFF",
          "line_height": 20,
          "max_lines": 6,
          "paddings": {
            "top": 8
          }
        },
        {
          "type": "text",
          "$text": "address",
          "font_size": 13,
          "line_height": 16,
          "text_color": "#99FFFFFF",
          "max_lines": 6,
          "paddings": {
            "top": 8
          }
        },
        {
          "type": "container",
          "orientation": "horizontal",
          "content_alignment_vertical": "center",
          "paddings": {
            "top": 8
          },
          "$items": "poi_stars"
        },
        {
          "type": "container",
          "orientation": "horizontal",
          "content_alignment_vertical": "center",
          "paddings": {
            "top": 11
          },
          "items": [
            {
              "type": "text",
              "$text": "time",
              "font_size": 11,
              "line_height": 12,
              "text_color": "#99FFFFFF",
              "max_lines": 1,
              "letter_spacing": 0.75,
              "width": {
                "type": "match_parent",
                "weight": 1
              }
            },
            {
              "type": "image",
              "image_url": "https://alicekit.s3.yandex.net/images_for_divs/chess.png",
              "width": {
                "type": "fixed",
                "value": 16
              },
              "height": {
                "type": "fixed",
                "value": 16
              }
            },
            {
              "type": "text",
              "$text": "distance",
              "font_size": 13,
              "text_color": "#99FFFFFF",
              "max_lines": 1,
              "width": {
                "type": "wrap_content",
                "weight": 1
              }
            }
          ]
        }
      ]
    },
    "gallery_tail_light": {
      "type": "container",
      "width": {
        "type": "fixed",
        "value": 104
      },
      "height": {
        "type": "match_parent"
      },
      "$action": "gallery_tail_action_link",
      "content_alignment_horizontal": "center",
      "content_alignment_vertical": "center",
      "items": [
        {
          "type": "image",
          "width": {
            "type": "fixed",
            "value": 40
          },
          "height": {
            "type": "fixed",
            "value": 40
          },
          "border": {
            "corner_radius": 20,
            "stroke": {
              "color": "#DCDEE0"
            }
          },
          "background": [
            {
              "type": "solid",
              "color": "#ffffff"
            }
          ],
          "placeholder_color": "#00ffffff",
          "image_url": "https://alicekit.s3.yandex.net/images_for_divs/chess.png"
        },
        {
          "type": "text",
          "$text": "tail_text_link",
          "font_size": 14,
          "text_color": "#6b7a80",
          "line_height": 16,
          "text_alignment_horizontal": "center",
          "height": {
            "type": "wrap_content"
          },
          "paddings": {
            "left": 0,
            "right": 0,
            "top": 10,
            "bottom": 0
          }
        }
      ]
