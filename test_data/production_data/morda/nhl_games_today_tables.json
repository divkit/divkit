  "templates": {
    "nhl_card": {
      "background": [
        {
          "color": "#ffffff",
          "type": "solid"
        }
      ],
      "height": {
        "type": "wrap_content"
      },
      "items": [
        {
          "$items": "card_title_items",
          "orientation": "horizontal",
          "type": "container"
        },
        {
          "$items": "tab_items_link",
          "switch_tabs_by_content_swipe_enabled": 0,
          "tab_title_style": {
            "font_weight": "medium"
          },
          "title_paddings": {
            "bottom": 8,
            "left": 12,
            "right": 12
          },
          "type": "tabs"
        },
        {
          "$text": "card_footer_text",
          "action": {
            "$url": "card_footer_action_link",
            "log_id": "footer"
          },
          "font_size": 12,
          "font_weight": "medium",
          "height": {
            "type": "wrap_content"
          },
          "letter_spacing": 0.5,
          "line_height": 14,
          "paddings": {
            "bottom": 15,
            "left": 16,
            "right": 16,
            "top": 15
          },
          "text_alignment_vertical": "bottom",
          "text_color": "#99000000",
          "type": "text"
        }
      ],
      "type": "container"
    },
    "nhl_games_today_tables": {
      "action": {
        "$url": "action_link",
        "log_id": "item"
      },
      "background": [
        {
          "color": "#ffffff",
          "type": "solid"
        }
      ],
      "border": {
        "corner_radius": 6,
        "stroke": {
          "color": "#E5E5E5"
        }
      },
      "height": {
        "type": "fixed",
        "unit": "sp",
        "value": 196
      },
      "items": [
        {
          "$text": "title_text",
          "type": "nhl_standing_tables_header"
        },
        {
          "$items": "nhl_standing_tables_items",
          "column_count": 5,
          "type": "grid"
        },
        {
          "delimiter_style": {
            "color": "#00000000",
            "orientation": "vertical"
          },
          "height": {
            "type": "match_parent",
            "weight": 1
          },
          "type": "separator"
        },
        {
          "$text": "button_text",
          "background": [
            {
              "color": "#0D000000",
              "type": "solid"
            }
          ],
          "border": {
            "corner_radius": 4
          },
          "font_size": 13,
          "font_weight": "regular",
          "line_height": 16,
          "max_lines": 1,
          "paddings": {
            "bottom": 8,
            "left": 12,
            "right": 12,
            "top": 8
          },
          "text_alignment_horizontal": "center",
          "text_color": "#CC000000",
          "type": "text"
        }
      ],
      "paddings": {
        "bottom": 12,
        "left": 12,
        "right": 12,
        "top": 14
      },
      "type": "container",
      "width": {
        "type": "fixed",
        "value": 328
      }
    },
    "nhl_standing_tables_big_cell": {
      "paddings": {
        "left": 6,
        "top": 8
      },
      "type": "nhl_standing_tables_text",
      "width": {
        "type": "match_parent",
        "weight": 1
      }
    },
    "nhl_standing_tables_big_cell_right": {
      "paddings": {
        "right": 6,
        "top": 8
      },
      "text_alignment_horizontal": "right",
      "type": "nhl_standing_tables_big_cell"
    },
    "nhl_standing_tables_header": {
      "font_size": 13,
      "font_weight": "regular",
      "line_height": 16,
      "max_lines": 1,
      "text_color": "#80000000",
      "type": "text"
    },
    "nhl_standing_tables_icon_cell": {
      "$image_url": "team_photo",
      "height": {
        "type": "fixed",
        "value": 16
      },
      "margins": {
        "top": 8
      },
      "scale": "fit",
      "type": "image",
      "width": {
        "type": "fixed",
        "value": 16
      }
    },
    "nhl_standing_tables_middle_cell": {
      "alignment_horizontal": "center",
      "paddings": {
        "left": 4,
        "right": 4,
        "top": 8
      },
      "type": "nhl_standing_tables_text",
      "width": {
        "type": "wrap_content"
      }
    },
    "nhl_standing_tables_middle_cell_comming": {
      "text_color": "#40000000",
