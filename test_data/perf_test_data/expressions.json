            {
              "type": "expressions_block",
              "expression": "@{mod(div((int1+int2)*int3, 15) - maxInteger()/minInteger()*abs(min(int1,int2)*max(int1,int2)),9)}"
            },
            {
              "type": "expressions_block",
              "expression": "@{round(mod(div((num1+num2)*num3, 15.0) - mod(maxNumber()-minNumber(),9.0) + abs(min(num1,num2)*max(num1,num2)),9.0))}"
            },
            {
              "type": "expressions_block",
              "expression": "@{floor((getColorRed(setColorRed(setColorGreen(setColorBlue(rgb(red,red,red),0.3),0.2),0.1)) + getColorGreen(setColorRed(setColorGreen(setColorBlue(rgb(red,red,red),0.3),0.2),0.1)) + getColorBlue(setColorRed(setColorGreen(setColorBlue(rgb(0.5,0.5,0.5),0.4),0.2),0.2))) * 10.0)}"
            },
            {
              "type": "expressions_block",
              "expression": "@{getHours(setHours(setDay(setMonth(setYear(addMillis(parseUnixTime(0),1000),2023),12),10),9))}"
            },
            {
              "type": "expressions_block",
              "expression": "@{testRegex(str_long, 'mqwertyui') ? (len(replaceAll(padStart(substring(str_long,0,1000),1100,'ca'),'a','b')) + lastIndex(toUpperCase(encodeUri(str_long)),'A')) / 1000 : ''}"
            },
            {
              "type": "expressions_block",
              "expression": "@{len('Lorem Ipsum @{substring(str_long,7,8)+substring('ab@{substring(str_long,11,12)}',2,3)} simply dummy text of the printing and typesetting industry.')-70}"
            },
            {
              "type": "expressions_block",
              "expression": "@{len(theme == 'light' ? '' : '')/1000}"
            },
            {
              "type": "expressions_block",
              "expression": "@{toInteger(getArrayString(arr,9)) - getArrayInteger(arr,3) + toInteger(getArrayNumber(arr,8)) - toInteger(getColorRed(getArrayColor(arr,7))) - getArrayOptInteger(arr,11,6)}"
            }
          ],
          "type": "container"
        }
      }
