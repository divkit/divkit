{
  "description": "Template link is overridden in derived template",
  "templates": {
    "base_text": {
      "type": "text",
      "$text": "content"
    },
    "titled_text": {
      "type": "base_text",
      "$text": "title"
    }
  },
  "card": {
    "log_id": "test",
    "states": [
      {
        "state_id": 0,
        "div": {
          "type": "titled_text",
          "content": "From content",
          "title": "From title"
        }
      }
    ]
  },
  "expected": {
    "card": {
      "log_id": "test",
      "states": [
        {
          "state_id": 0,
          "div": {
            "type": "text",
            "text": "From title"
          }
        }
      ]
    }
  }
}
