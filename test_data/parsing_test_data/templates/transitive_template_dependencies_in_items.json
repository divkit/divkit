{
  "description": "Transitive template dependencies in nested items",
  "templates": {
    "base_text": {
      "type": "text",
      "font_size": 14,
      "$text": "text_value"
    },
    "list_item": {
      "type": "container",
      "orientation": "vertical",
      "items": [
        {
          "type": "base_text",
          "$text_value": "title"
        }
      ]
    }
  },
  "card": {
    "log_id": "test",
    "states": [
      {
        "state_id": 0,
        "div": {
          "type": "list_item",
          "title": "Resolved through chain"
        }
      }
    ]
  },
  "expected": {
    "error_count": 1,
    "card": {
      "log_id": "test",
      "states": [
        {
          "state_id": 0,
          "div": {
            "type": "container",
            "orientation": "vertical",
            "items": []
          }
        }
      ]
    }
  }
}
