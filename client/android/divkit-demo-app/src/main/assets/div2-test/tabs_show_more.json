  "templates": {
    "customTabs": {
      "type": "container",
      "height": {
        "type": "match_parent"
      }
    },
    "tabPageWithItem": {
      "type": "state",
      "states": [
        {
          "div": {
            "type": "container",
            "$items": "closedItems"
          },
          "$state_id": "closedStateId"
        },
        {
          "div": {
            "type": "tabStoriesBase",
            "height": {
              "type": "wrap_content"
            },
            "margins": {
              "left": 6,
              "right": 6
            },
            "paddings": {
              "top": 0,
              "bottom": 0
            },
            "$items": "extraItems"
          },
          "$state_id": "openedStateId"
        }
      ]
    },
    "tabStoriesBase": {
      "type": "container",
      "paddings": {
        "top": 5,
        "bottom": 5
      },
      "margins": {
        "left": 4,
        "right": 4
      },
      "border": {
        "corner_radius": 20
      },
      "height": {
        "type": "match_parent"
      },
      "background": [
        {
          "type": "solid",
          "color": "#F3F3F2"
        }
      ]
    },
    "tabState": {
      "type": "state",
      "states": [
        {
          "div": {
            "type": "container",
            "items": [
              {
                "type": "text",
                "margins": {
                  "top": 16,
                  "left": 20,
                  "right": 20,
                  "bottom": 11
                },
                "font_size": 14,
                "font_weight": "medium",
                "line_height": 18,
                "text_alignment_horizontal": "center",
                "$text": "buttonText",
                "$action": "showMoreAction"
              }
            ]
          },
          "$state_id": "buttonStateId"
        },
        {
          "div": {
            "type": "container",
            "$items": "stateItems"
          },
          "$state_id": "storiesStateId"
        }
      ]
    },
    "tabStories": {
      "type": "tabStoriesBase",
      "background": [
        {
          "type": "solid",
          "color": "#FFF"
        }
      ]
    },
    "tabItem": {
      "type": "text",
      "margins": {
        "right": 12,
        "top": 16,
        "bottom": 16,
        "left": 12
      },
      "line_height": 20,
      "font_size": 16,
      "max_lines": 3,
      "background": [
        {
          "color": "#E0E2E6",
          "type": "solid"
        }
      ],
      "border": {
        "corner_radius": 5
      },
      "text": "   "
    }
  }
}
