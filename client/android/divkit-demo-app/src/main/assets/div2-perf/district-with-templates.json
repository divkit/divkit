        {
          "type": "image",
          "width": {
            "type": "fixed",
            "value": 24
          },
          "height": {
            "type": "fixed",
            "value": 24
          },
          "margins": {
            "right": 8
          },
          "border": {
            "corner_radius": 12
          },
          "$image_url": "userpic_url"
        },
        {
          "type": "post_content_text",
          "font_size": 14,
          "line_height": 18,
          "max_lines": 1,
          "text_color": "#333333",
          "$text": "author_and_location",
          "$ranges": "location_spans"
        }
      ]
    },
    "post_item_footer": {
      "type": "container",
