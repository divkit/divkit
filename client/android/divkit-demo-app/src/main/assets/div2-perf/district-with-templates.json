  "templates": {
    "header": {
      "type": "container",
      "orientation": "horizontal",
      "items": [
        {
          "type": "text",
          "height": {
            "type": "fixed",
            "value": 48
          },
          "paddings": {
            "left": 16,
            "right": 16
          },
          "font_size": 18,
          "font_weight": "medium",
          "line_height": 24,
          "text_alignment_vertical": "center",
          "text_color": "#CC000000",
          "$text": "header_text"
        },
        {
          "type": "image",
          "width": {
            "type": "fixed",
            "value": 48
          },
          "height": {
            "type": "fixed",
            "value": 48
          },
          "paddings": {
            "left": 16,
            "top": 12,
            "right": 8,
            "bottom": 12
          },
          "placeholder_color": "#00000000",
          "image_url": "file:///android_asset/images/district/menu.png",
          "$action": "menu_action"
        }
      ]
    },
    "button": {
      "type": "text",
      "paddings": {
        "left": 12,
        "top": 8,
        "right": 12,
        "bottom": 8
      },
      "background": [
        {
          "type": "solid",
          "color": "#FFDC60"
        }
      ],
      "border": {
        "corner_radius": 4
      },
      "font_size": 14,
      "max_lines": 1,
      "text_alignment_horizontal": "center",
      "text_alignment_vertical": "center"
    },
    "post_content_text": {
      "type": "text",
      "paddings": {
        "right": 24
      }
    },
    "post_title": {
      "type": "post_content_text",
      "font_size": 16,
      "font_weight": "medium",
      "line_height": 20,
      "max_lines": 3
    },
    "post_item_header": {
      "type": "container",
      "height": {
        "type": "fixed",
        "value": 48
      },
      "orientation": "horizontal",
      "content_alignment_vertical": "center",
